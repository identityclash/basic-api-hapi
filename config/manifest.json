{
	"server": {},
	"connections": [
		{
			"port": 3000,
			"labels": ["api"]
		}
	],
	"registrations": [
		{
			"plugin": {
				"register": "../modules/database"
			}
		},
		{
			"plugin": {
				"register": "../modules/api-service"
			}
		},
		{
			"plugin": {
				"register": "good",
				"options": {
					"reporters": {
						"console": [
							{
								"module": "good-squeeze",
								"name": "Squeeze",
								"args": [{
									"ops": "*",
									"log": "*",
									"error": "*",
									"response": "*"
								}]
							},
							{ "module": "good-console" },
							"stdout"
						]
					}
				}
			}
		}
	]
}